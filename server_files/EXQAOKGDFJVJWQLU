// cat - Print contents of file
// Programmed by Trigex, 2017, licensed under the Apache License version 2
$cat_check = Functions.file_exists(-upload /bin/cat.code)
if $cat_check = 1
    $cat_data = Functions.file_load_text(-upload /bin/cat.code | dev)
    Windows.HTerm.List2.AddItem $cat_data
else
    Windows.HTerm.List2.AddItem File -upload /bin/cat.code not found
endif